    <src1 type="integer">1</src1>
  </tac>
  <tac opcode="PUSH" order="2">
    <src1 type="integer">2</src1>
  </tac>
  <tac opcode="POP" order="3">
    <dst type="variable">popVar</dst>
  </tac>
